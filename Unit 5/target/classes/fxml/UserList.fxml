<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserList">

    <TableView fx:id="userTable" layoutX="20" layoutY="57.0" prefHeight="323.0" prefWidth="560">
        <columns>
            <TableColumn fx:id="colId" prefWidth="60.0" text="ID" />
            <TableColumn fx:id="colUsername" prefWidth="156.0" text="Username" />
            <TableColumn fx:id="colPassword" prefWidth="343.0" text="Password" />
        </columns>
    </TableView>
   <ButtonBar layoutX="20.0" layoutY="14.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="280.0">
     <buttons>
         <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="113.0" text="Delete User" onAction="#deleteUser"/>
         <Button mnemonicParsing="false" text="Edit User" onAction="#editUser"/>
       <Button mnemonicParsing="false" text="Add User" onAction="#addUser"/>
     </buttons>
   </ButtonBar>

</AnchorPane>
